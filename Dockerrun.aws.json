{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "backend",
      "image": "2donny/4oclock-backend",
      "hostname": "backend",
      "essential": false,
      "memory": 1024,
      "portMappings": [
        {
          "hostPort": 3080,
          "containerPort": 3080
        }
      ]
    },
    {
      "name": "nginx",
      "image": "2donny/4oclock-nginx",
      "hostname": "nginx",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": ["backend"],
      "memory": 512
    }
  ]
}
